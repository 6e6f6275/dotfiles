#!/bin/bash

function black {
  printf "\e[30m$*\e[m";
}

function red {
  printf "\e[31m$*\e[m";
}

function green {
  printf "\e[32m$*\e[m";
}

function yellow {
  printf "\e[33m$*\e[m";
}

function blue {
  printf "\e[34m$*\e[m";
}

function magenta {
  printf "\e[35m$*\e[m";
}

function cyan {
  printf "\e[36m$*\e[m";
}

function white {
  printf "\e[37m$*\e[m";
}

cyan hello
